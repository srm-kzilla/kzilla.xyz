<!-- Global site tag (gtag.js) - Google Analytics -->
<%
getUrl = function(url) {
  if (!/^https?:\/\//i.test(url)) {
    url = 'http://' + url;
  }
  return url
}
%>
<script src="https://www.googletagmanager.com/gtag/js?id=UA-163949456-1"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag() { dataLayer.push(arguments); }
  gtag('js', new Date());
  gtag('config', 'UA-163949456-1');
</script>


<script>
  setTimeout(() => {
    window.open("<%=getUrl(data.longUrl)%>", "_self", true)
  }, 3000);
</script>