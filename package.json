{
  "name": "kzilla.xyz-server",
  "version": "0.0.1",
  "scripts": {
    "preinstall": "which link-module-alias && link-module-alias clean || echo",
    "start": "node build/server",
    "tsc": "tsc",
    "dev": "cross-env NODE_ENV=development tsc-watch --onSuccess \"node ./build/server.js\"",
    "build": "yarn run tsc && yarn run copy",
    "copy": "cp -r ./api/views/ ./build/api/views/",
    "postinstall": "link-module-alias"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.7",
    "@types/express-rate-limit": "^5.0.0",
    "@types/hapi__joi": "^17.1.4",
    "@types/helmet": "^0.0.47",
    "@types/mongodb": "^3.5.25",
    "@types/node-cache": "^4.2.5",
    "@types/pino": "^7.0.5",
    "@types/request-ip": "^0.0.35",
    "cross-env": "^7.0.3",
    "link-module-alias": "^1.2.0",
    "tsc-watch": "^5.0.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "express-csp-header": "^4.0.0",
    "express-rate-limit": "^5.1.3",
    "googleapis": "^58.0.0",
    "helmet": "^4.0.0",
    "metaget": "^1.0.7",
    "moment": "^2.27.0",
    "mongodb": "^3.5.6",
    "node-cache": "^5.1.2",
    "pino": "^8.17.1",
    "pino-pretty": "^8.0.0",
    "request-ip": "^2.1.3",
    "typescript": "^3.9.7"
  },
  "_moduleAliases": {
    "~root": ".",
    "@controllers": "./api/controllers",
    "@middlewares": "./api/middlewares",
    "@models": "./api/models",
    "@routes": "./api/routes",
    "@services": "./api/services",
    "@utils": "./api/utils",
    "@views": "./api/views"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0 <17.0.0"
  }
}
